<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
        <title>{% block title %}Welcome!{% endblock %}</title>
       <link rel="icon" href="#">
        
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
            {# {% include 'gp-tracker/layout/_stylesheet.html.twig' %} #}
        {% endblock %}
        

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
            {# {% include 'gp-tracker/layout/_script.html.twig'%} #}
        {% endblock %}
    </head>
    <body >
    
        {% if app.request.attributes.get('_route') not in ['app_login', 'app_register', 'non_verified_user_page'] %}
            {% include 'gp-tracker/layout/_header.html.twig' %}
        {% endif %}

        {% block body %}{% endblock %}

        {% if app.request.attributes.get('_route') not in ['app_login', 'app_register', 'non_verified_user_page'] %}
            {% include 'gp-tracker/layout/_footer.html.twig' %}
        {% endif %}

    </body>
</html>